+++ gcc/ada/gcc-interface/Makefile.in	2021-09-17 02:16:17.169904292 +0300
@@ -613,6 +613,7 @@
 	    $(OSCONS_CPP) ; \
 	    $(OSCONS_EXTRACT) ; \
 	    ../bldtools/oscons/xoscons s-oscons)
+	touch -r $(fsrcpfx)ada/gsocket.h $@
 
