# bash completion for prime

_prime()
{
    local cur prev words cword split
    _init_completion -s || return
    for (( i=1; i <= cword; i++ )); do
        if [[ ${words[i]} != -* ]]; then
            local root_command=${words[i]}
            _command_offset $i
            return
        fi
	done
} &&
complete -F _prime prime

# ex: filetype=sh
