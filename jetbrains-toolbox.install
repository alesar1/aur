TITLE_MSG=$' \e[4;1;32;40mJetBrains Toolbox\e[0m'

INSTALL_MSG=$' \e[1mRememeber to quit any instance of the toolbox which
 may be running, then start from the command prompt by running the command:
 $ jetbrains-toolbox\e[0m'

post_install() {
	echo -e "\n \e[33mNOTE\e[39m:$TITLE_MSG"
	echo -e "${INSTALL_MSG}\n"
}

post_upgrade() {
	post_install
}
