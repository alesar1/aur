    \end{tableofcontents}
    \clearpage %beginnt neue Seite, damit Seitennummern nicht im Inhaltsverzeichnis starten
    \pagenumbering{arabic} %reaktiviert Seitennummern
    \setcounter{page}{1} %setzt Seitennummer auf 1
    }
}

% Abgaben-Layout
\newcommand{\cispage}[3]{
    {\pagenumbering{arabic}
    \begin{center}
        \begin{center}
            {\scshape\large #1 - #2\par} % #Modulname - #Blatt
            {#3\par} % #Namen
        \end{center}
        {\vspace{-3cm}\cistextlogo[0.7]\vspace{2cm}} % (0.7 früher 0.025)
    \end{center}}
}

% Abgaben-Abschnitte
\newcommand{\cisection}[2][]{
    \ifthenelse{
        \equal{\string #2}{\string H}
    }{
        {\large \textbf{Hausaufgaben #1}}
        \cisbr
    }{
        \ifthenelse{
            \equal{\string #2}{\string P}
        }{
            {\large \textbf{Präsenzaufgaben #1}}
            \cisbr
        }{
            {\large \textbf{#2 #1}}
            \cisbr
        }
    }
}

