% Erste Seite, formellere Dokumente
\NewDocumentCommand{\cisdoc}{O{}D(){}mm}{
    {\pagenumbering{gobble} %deaktiviert Seitennummern
    \begin{titlepage}
        \centering
        {\cistextlogo[3.8]\par} %Logo % (3.8 früher 0.15)
        \vspace{1cm}
        {\scshape\Large #4\par} %Haupttitel
        {\Large\bfseries #1\par} %Untertitel (optional)
        \vspace{0.5cm}
        {\Large\itshape #3\par} %Namen
        \today
        \vfill
        {\ifx#2\empty\else\includegraphics[width=0.8\textwidth,height=0.4\textwidth,keepaspectratio]{#2}\fi\par} %optionales, zweites Logo
        \vspace{0.5cm}
    \end{titlepage}
