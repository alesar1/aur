pkgname=fluxbox-style-jet-gray
pkgver=20160608
pkgrel=1
pkgdesc="Fluxbox style for Jet-Gray and Zukitwo Dark"
url="https://github.com/corebob/fluxbox-style-jet-gray"
license=('CCPL')
arch=('any')
source=("https://github.com/corebob/fluxbox-style-jet-gray/archive/v1.0.zip")
md5sums=('7da58c6f884e2fac781a6563dbbaee15')

package() {
  mkdir -p "${pkgdir}/usr/share/fluxbox/styles/Jet-Gray"
  cp -r ${srcdir}/fluxbox-style-jet-gray-1.0/Jet-Gray "${pkgdir}/usr/share/fluxbox/styles/"
}

