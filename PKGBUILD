package() {
    install -d ${pkgdir}/usr/share/fonts/${pkgname}/
    for font in ${srcdir}/*.otf; do
        install -m644 $font ${pkgdir}/usr/share/fonts/${pkgname}/${font/${srcdir}\/${_v}-/}
    done
    install -D -m644 ${_v}-OFL.txt ${pkgdir}/usr/share/licenses/${pkgname}/OFL.txt
}
