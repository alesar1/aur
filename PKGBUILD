sha256sums=('c1cdc8de5d9b41ab1fd301e5dd2b41939b70c9c5330f654c355d254150fb364f')

package(){
   mkdir -p "${pkgdir}"/usr/share/keepass/Languages
   install -m644 Spanish.lngx "${pkgdir}"/usr/share/keepass/Languages
}
