build() {
	cd "${_pkgname}-${pkgver}"
	./configure --prefix=/usr --sysconfdir=/etc
}
package() {
	cd "${_pkgname}-${pkgver}"
	make DESTDIR="${pkgdir}/" samples-conf
}
