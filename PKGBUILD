pkgname=i3-auto-arrange
pkgver=1.0.2
pkgrel=1
pkgdesc="This is a simple Shell script that will automatically rearrange i3 workspaces"
arch=('any')
url="https://github.com/denis-ismailaj/i3-auto-arrange"
license=('GPL3')
depends=('i3-wm' 'jq')

package() {
	sudo cp "$startdir/i3-auto-arrange" /usr/bin
}

