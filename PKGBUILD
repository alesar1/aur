  install -Dm644 thunderbird-beta-i18n-$pkgver-uz.xpi \
    "$pkgdir/usr/share/mozilla/extensions/{3550f703-e582-4d05-9a08-453d09bdfdc6}/langpack-uz@thunderbird.mozilla.org.xpi"
}

