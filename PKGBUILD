source=("${_source[@]/#/${_base_source}}")
package (){
  cd "$srcdir"
  _scriptsdir="$pkgdir"/usr/share/gimp/2.0/scripts
  install -dm755 "$_scriptsdir"
  for i in *.py; do
    install -Dm644  $i "$_scriptsdir"/$i
  done
  install -Dm644 readme.md "$pkgdir/usr/share/doc/$pkgname/readme.md"
}
