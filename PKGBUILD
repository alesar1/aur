# Maintainer: scan
pkgname=keepass-es
pkgver=2.48
pkgrel=1
pkgdesc="KeePass EspaÃ±ol"
license=('GPL2')
depends=("keepass>=${pkgver}")
makedepends=('unzip')
url="http://keepass.info/translations.html"
source=(http://downloads.sourceforge.net/keepass/KeePass-${pkgver}-Spanish.zip)
