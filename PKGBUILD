pkgname=brave-browser-git
_pkgname=browser-laptop
pkgver=0.8.3.30
pkgrel=1
pkgdesc="A web browser that stops ads and trackers by default. Master branch. Dummy package, pulls in new PKGBUILD."
arch=('x86_64') # Upstream supports x86_64 only
url="https://www.brave.com/"
license=('custom:MPL2' 'BSD' 'Apache' 'custom:others')
groups=('networking')
depends=('brave-git')

package() {
  echo "Empty package that pulls new package brave-git. Falling in line with opera and vivaldi."
}

