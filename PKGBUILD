pkgname=dvb-firmware-pack
pkgver=1
pkgrel=1
pkgdesc="Linux TV's firmware package"
arch=('x86_64')
url='https://linuxtv.org'
license=('GPL' 'Custom:https://raw.githubusercontent.com/OpenELEC/dvb-firmware/master/firmware/README.as102')
makedepends=('tar' 'git')
source=("https://linuxtv.org/downloads/firmware/dvb-firmwares.tar.bz2"
        "git+https://github.com/OpenELEC/dvb-firmware.git")
