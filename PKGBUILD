license="cc-by-nc-nd-4.0"
arch=any
source="http://esclapion.free.fr/AUR/$pkgname-$pkgver.tar.gz"
md5sums=('f946f3a478d330e9f131ad8f2807256c')

package(){
  mkdir -p $pkgdir/usr/share/icons
  cp -a $srcdir/Plataro $pkgdir/usr/share/icons
}
