# Install file for brother-ql700-cupswrapper

post_install() {
	/opt/brother/PTouch/ql700/cupswrapper/cupswrapperql700pt1
	systemctl restart org.cups.cupsd.service
}

pre_remove() {
	/opt/brother/PTouch/ql700/cupswrapper/cupswrapperql700pt1 -e
}

post_remove() {
	systemctl restart org.cups.cupsd.service
}

post_upgrade() {
	systemctl restart org.cups.cupsd.service
}
