From 38c640c5c6845e296d6dc69e74866cf7cb25fccc Mon Sep 17 00:00:00 2001
From: Tim Dunham <tdunham@grc.internal>
Date: Thu, 2 Apr 2020 10:29:50 -0400
Subject: [PATCH 1/2] fix jar build issues

---
 build.gradle | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/build.gradle b/build.gradle
index 9d9a7fb..60cac47 100644
--- a/build.gradle
+++ b/build.gradle
@@ -1,11 +1,11 @@
 apply plugin: 'java'
-jar.enabled = false
+jar.enabled = true
 
 project.buildDir = 'bin'
 
 sourceSets {
   main {
-    java.outputDir = file('bin')
+    java.outputDir = file('compiled')
     java {
       srcDirs = ['src']
     }    

