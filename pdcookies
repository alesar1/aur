#!/usr/bin/sh


cookies_db="$1"

tmpdir=$(mktemp -d)
cp "$1" $tmpdir

sqlite3 -newline '; ' -separator = "$tmpdir/cookies.sqlite" "select name, value  from moz_cookies where host = '.baidu.com' and name = 'BDUSS' or host = '.pan.baidu.com' and name = 'STOKEN'"

rm $tmpdir/cookies.sqlite
rmdir $tmpdir
