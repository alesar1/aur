post_upgrade(){
    echo -e "\033[31m If you're upgrading from below 2.1.0-3 \033[0m"
    echo -e "\033[31m Please move /usr/bin/data to /var/lib/alist \033[0m"
    echo "You can run "
    echo -e "\033[31m sudo systemctl stop alist \033[0m"
    echo -e "\033[31m sudo mv /usr/bin/data /var/lib/alist \033[0m"
    echo -e "\033[31m sudo systemctl start alist \033[0m"
}
