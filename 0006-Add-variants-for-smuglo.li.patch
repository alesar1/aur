+"""infinityctl variant for smuglo.li (onion)"""
+
+import os
+
+import infinitychan
+import infinityctl
+
+infinitychan.dom = "http://bhm5koavobq353j54qichcvzr6uhtri6x4bjjy4xkybgvxkzuslzcqid.onion"
+infinitychan.mod = infinitychan.dom + "/mod.php?"
+infinitychan.sys = infinitychan.dom
