From: Andrew Apted <ajapted@gmail.com>
Date: Fri, 22 Dec 2017 12:41:03 +1100
Subject: [PATCH] Makefile, Makefile.macos : fixed for "prefabs" directory
 removal.

---
 Makefile       | 5 -----
 Makefile.macos | 5 -----
 2 files changed, 10 deletions(-)

diff --git a/Makefile b/Makefile
index b4fe319ee..2588f2082 100644
--- a/Makefile
+++ b/Makefile
@@ -257,11 +257,6 @@ install: stripped
 	chown -R root $(SCRIPT_DIR)/games
 	chmod -R g-s  $(SCRIPT_DIR)/games
 	#
-	rm -Rf $(SCRIPT_DIR)/prefabs
-	cp -a prefabs $(SCRIPT_DIR)/prefabs
-	chown -R root $(SCRIPT_DIR)/prefabs
-	chmod -R g-s  $(SCRIPT_DIR)/prefabs
-	#
 	xdg-desktop-menu  install --novendor misc/oblige.desktop
 	xdg-icon-resource install --novendor --size 32 misc/oblige.xpm
 
diff --git a/Makefile.macos b/Makefile.macos
index 8c8647acf..e1ceee58a 100644
--- a/Makefile.macos
+++ b/Makefile.macos
@@ -283,11 +283,6 @@ install: stripped
 	cp -a games $(SCRIPT_DIR)/games
 	chown -R root $(SCRIPT_DIR)/games
 	chmod -R g-s  $(SCRIPT_DIR)/games
-	#
-	rm -Rf $(SCRIPT_DIR)/prefabs
-	cp -a prefabs $(SCRIPT_DIR)/prefabs
-	chown -R root $(SCRIPT_DIR)/prefabs
-	chmod -R g-s  $(SCRIPT_DIR)/prefabs
 
 uninstall:
 	rm -v $(PREFIX)/bin/oblige
