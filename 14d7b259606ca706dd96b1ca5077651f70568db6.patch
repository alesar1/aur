From 14d7b259606ca706dd96b1ca5077651f70568db6 Mon Sep 17 00:00:00 2001
From: Bilal Elmoussaoui <belmouss@redhat.com>
Date: Sat, 12 Mar 2022 22:06:04 +0100
Subject: [PATCH] fix post librsvg release

---
 src/icons/icon.rs | 4 ++--
 src/project.rs    | 2 +-
 2 files changed, 3 insertions(+), 3 deletions(-)

diff --git a/src/icons/icon.rs b/src/icons/icon.rs
index a0a3412..31a1da2 100644
--- a/src/icons/icon.rs
+++ b/src/icons/icon.rs
@@ -79,8 +79,8 @@ impl Icon {
             cairo::Rectangle {
                 x: 0.0,
                 y: 0.0,
-                width: dimensions.width.unwrap().length as f64,
-                height: dimensions.height.unwrap().length as f64,
+                width: dimensions.width.length as f64,
+                height: dimensions.height.length as f64,
             }
         };
         let (rect, _) = renderer.geometry_for_layer(Some(&id), &viewport)?;
diff --git a/src/project.rs b/src/project.rs
index e2b9ae9..ebf0538 100644
--- a/src/project.rs
+++ b/src/project.rs
@@ -44,7 +44,7 @@ impl Project {
 
         let (width, height) = {
             let dimensions = renderer.intrinsic_dimensions();
-            (dimensions.width.unwrap().length, dimensions.height.unwrap().length)
+            (dimensions.width.length, dimensions.height.length)
         };
 
         // Create cache directory
