{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchDatasources": ["github-releases"],
      "matchPackageNames": ["lDeFiCh/app"],
      "automerge": false
    }
  ],
  "regexManagers": [
    {
      "fileMatch": ["^PKGBUILD$"],
      "matchStrings": ["pkgver=(?<currentValue>.*?)\n"]
    }
  ]
}
