#!/bin/bash

. /etc/rc.conf
. /etc/rc.d/functions

# source application-specific settings
[ -f /etc/conf.d/c2ctl ] && . /etc/conf.d/c2ctl

case "$1" in
	start)
		stat_busy "Setting processor parameters"
		/usr/bin/c2ctl $CORES $FID $VID >/dev/null 2>&1
		stat_done
		;;
	*)
		echo "usage: $0 {start}"
esac
exit 0
