post_install() {
  # Setup install location in config file.
  sed -i 's/BINDIFF_DIR/\/opt\/zynamics\/BinDiff\//' /etc/opt/zynamics/BinDiff/bindiff_core.xml

  # Add application launcher to desktop DB
  update-desktop-database -q
}

post_upgrade() {
  post_install
}

post_remove() {
  update-desktop-database -q
}
