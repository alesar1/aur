@@ -1,4 +1,4 @@
-file(GLOB _qt5qml_other_plugins "${CMAKE_CURRENT_LIST_DIR}/Qt5Qml_*Factory.cmake")
+file(GLOB _qt5qml_other_plugins "${CMAKE_CURRENT_LIST_DIR}/*Qt5Qml_*Factory.cmake")
 
 foreach(_other_plugin ${_qt5qml_other_plugins})
     include(${_other_plugin} OPTIONAL)
