# chooser=textmenu
prompt

default=arch
timeout=10

image=/EFI/arch/vmlinuz-arch.efi
	label=arch
	append="root=PARTUUID=XXXXXXXXXX rootfstype=ext4 add_efi_memmap"
	initrd=/EFI/arch/initramfs-arch.img

image=/EFI/arch/vmlinuz-arch.efi
	label=arch-fallback
	append="root=PARTUUID=XXXXXXXXXX rootfstype=ext4 add_efi_memmap"
	initrd=/EFI/arch/initramfs-arch-fallback.img
